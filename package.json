{
  "name": "busy-bee-recipes",
  "version": "0.1.0",
  "description": "Cooking recipes for busy bees",
  "main": "server/main.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node server/seed.js",
    "start": "node server/main.js",
    "start:dev": "npm run build:dev & npm run start-server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "JWT=shh nodemon main.js -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV=test JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch",
    "test:dev:models": "npm run test:dev -- --grep=model",
    "test:dev:routes": "npm run test:dev -- --grep=route"
  },
  "author": "Amy Tran",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "bootstrap": "^4.6.0",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.9.1",
    "node-sass": "^5.0.0",
    "pg": "^8.5.1",
    "reactstrap": "^8.9.0",
    "sass-loader": "^11.0.1",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^5.2.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "esm": "^3.2.25",
    "file-loader": "^6.2.0",
    "mocha": "^6.2.3",
    "nodemon": "^1.19.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "style-loader": "^2.0.0",
    "supertest": "^4.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  }
}
